{
  "name": "dmt-software/ab-middleware",
  "description": "ab testing middleware compatible with psr",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "DMT\\AbMiddleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DMT\\AbMiddleware\\": "test/"
    }
  },
  "authors": [
    {
      "name": "lingonl",
      "email": "lingo@de-mes.nl"
    }
  ],
  "require": {
    "php": "^8.3",
    "psr/http-server-middleware": "^1.0",
    "dflydev/fig-cookies": "^3.1",
    "google/analytics-admin": "^0.24.0"
  },
  "require-dev": {
    "ext-pcov": "*",
    "phpunit/phpunit": "^11.4",
    "squizlabs/php_codesniffer": "^3.10",
    "guzzlehttp/psr7": "^2.7",
    "twig/twig": "^3.14"
  },
  "suggest": {
    "twig/twig": "If you want to use the twig helper"
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs src/ test/ --standard=PSR12",
    "phpunit": "phpunit"
  }
}
